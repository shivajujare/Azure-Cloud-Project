{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "retail218-ADF"
		},
		"ls_retailadls218_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_retailadls218'"
		},
		"kvs_retail_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvs-retail.vault.azure.net/"
		},
		"ls_retailadls218_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retailadls218.dfs.core.windows.net/"
		},
		"orders_trigger_properties_pl_orders_dbws_parameters_pl_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"orders_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f320e263-75fd-4b5b-906b-369f2ba80f6c/resourceGroups/rg-retail-dev/providers/Microsoft.Storage/storageAccounts/retailadls218"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "retail218-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_retailadls218_accountKey": {
						"value": "[parameters('ls_retailadls218_accountKey')]"
					},
					"kvs_retail_properties_typeProperties_baseUrl": {
						"value": "[parameters('kvs_retail_properties_typeProperties_baseUrl')]"
					},
					"ls_retailadls218_properties_typeProperties_url": {
						"value": "[parameters('ls_retailadls218_properties_typeProperties_url')]"
					},
					"orders_trigger_properties_pl_orders_dbws_parameters_pl_filename": {
						"value": "[parameters('orders_trigger_properties_pl_orders_dbws_parameters_pl_filename')]"
					},
					"orders_trigger_properties_typeProperties_scope": {
						"value": "[parameters('orders_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}